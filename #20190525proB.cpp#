#include<bits/stdc++.h>
using namespace std;
int main(){
  int n,m,mapp[20][20]={},jx,jy,signnum,flag=0,rx,ry;
  char sign;
  while(1){
    cin >> n;
    if(n==0) break;
    for(int i=0;i<n;i++){
      cin >> jx >> jy;
      mapp[jx-1][jy-1]=1; 
    }
    
    cin >> m;
    rx = 9,ry=9;
    for(int i=0;i<m;i++){
      
      cin >> sign >> signnum;
      switch(sign){
      case 'N':
	for(int i=0;i<=signnum;i++){
	  mapp[rx][ry+i]=0;
	}
	ry += signnum;
	break;
      case 'E':
	for(int i=0;i<=signnum;i++){
	  mapp[rx+i][ry]=0;
	}
	rx += signnum;
	break;	
      case 'S':
	for(int i=0;i<=signnum;i++){
	  mapp[rx][ry-i]=0;
	}
	ry -= signnum;
	break;	
      case 'W':
	for(int i=0;i<=signnum;i++){
	  mapp[rx-i][ry]=0;
	}
	rx -= signnum;
	break;	
      }
      }
    for(int j=0;j<20;j++){
      for(int i=0;i<20;i++){
	//cout << " " <<mapp[i][j];
	if(mapp[i][j]==0) flag++;
	mapp[i][j] = 0;
      }
    }
    if(flag==400) cout << "Yes" << endl;
    else cout << "No" << endl;
    flag =0;
    
    
  }
  return 0;
}
